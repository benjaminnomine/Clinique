{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@fortawesome/fontawesome-free@5.14.0",
      "destination": "wwwroot/lib/fortawesome/fontawesome-free/"
    },
    {
      "provider": "cdnjs",
      "library": "fullcalendar@3.10.2",
      "destination": "wwwroot/lib/fullcalendar/"
    },
    {
      "provider": "cdnjs",
      "library": "fullcalendar-scheduler@1.10.1",
      "destination": "wwwroot/lib/fullcalendar-scheduler/"
    },
    {
      "provider": "cdnjs",
      "library": "Chart.js@2.9.3",
      "destination": "wwwroot/lib/Chart.js/"
    }
  ]
}